ucts();\n    }, [page]);\n\n    useEffect(() => {\n        getProductsCount().then((res) => setProductsCount(res.data));\n    }, []);\n\n    const loadAllProducts = () => {\n        setLoading(true);\n        // sort, order, limit\n        getProducts(\"sold\", \"desc\", page).then((res) => {\n            setProducts(res.data);\n            setLoading(false);\n        });\n    };\n\n    return (\n        <>\n            <div className=\"container\">\n                {loading ? (\n                    <LoadingCard count={3} />\n                ) : (\n                    <div className=\"row\">\n                        {products.map((product) => (\n                            <div key={product._id} className=\"col-md-4\">\n                                <ProductCard product={product} />\n                            </div>\n                        ))}\n                    </div>\n                )}\n            </div>\n\n            <div className=\"row\">\n                <nav className=\"col-md-4 offset-md-4 text-center pt-5 p-3\">\n                    <Pagination\n                        current={page}\n                        total={(productsCount / 3) * 10}\n                        onChange={(value) => setPage(value)}\n                    />\n                </nav>\n            </div>\n        </>\n    );\n};\n\nexport default BestSellers;\n","/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/pages/Product.js",["211","212"],"/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/components/cards/SingleProduct.js",["213","214","215","216","217","218"],"/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/components/cards/ProductListItems.js",[],"/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/components/modal/RatingModal.js",["219"],"/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/pages/auth/Login.js",[],["220","221"],"/Users/dontrellwashington/Documents/mern/mern-ecommerce/client/src/